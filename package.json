  "scripts": {
    "dev": "concurrently \"npm run client:watch\" \"npm run server:watch\"",
    "start": "concurrently \"npm run client:build\" \"npm run server:start\"",
    "client:build": "webpack --config ./webpack.production.config.js",
    "client:watch": "webpack -w",
    "server:start": "nodemon ./build/app.bundle.js",
    "server:build": "webpack --config ./webpack.server.production.config.js app.js",
    "server:watch": "webpack -w --config ./webpack.server.config.js app.js"
  },
